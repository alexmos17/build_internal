[
  {
    "cmd": [
      "git",
      "diff",
      "--cached",
      "--name-only"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "git diff to analyze patch",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/build/gyp_chromium",
      "--analyzer",
      "yy"
    ],
    "env": {
      "GYP_DEFINES": "component=static_library gomadir='[BUILD]/goma' target_arch=x64 use_goma=1"
    },
    "name": "analyze",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_TEXT@No compile necessary@@@"
    ]
  }
]