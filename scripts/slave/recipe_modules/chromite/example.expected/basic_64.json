[
  {
    "cmd": [
      "[DEPOT_TOOLS_ROOT]/repo",
      "init",
      "-u",
      "https://chromium.googlesource.com/chromiumos/manifest.git",
      "--repo-url",
      "https://chromium.googlesource.com/external/repo.git"
    ],
    "name": "repo init",
    "seed_steps": [
      "repo init",
      "repo sync",
      "setup board",
      "build packages"
    ]
  },
  {
    "cmd": [
      "[DEPOT_TOOLS_ROOT]/repo",
      "sync"
    ],
    "name": "repo sync"
  },
  {
    "cmd": [
      "python",
      "-u",
      [
        "[SLAVE_BUILD_ROOT]/chromite/bin/cros_sdk",
        "--cache-dir",
        ".cache",
        "--",
        "./setup_board",
        "--board",
        "amd64-generic"
      ]
    ],
    "name": "setup board"
  },
  {
    "cmd": [
      "python",
      "-u",
      [
        "[SLAVE_BUILD_ROOT]/chromite/bin/cros_sdk",
        "--",
        "./build_packages",
        "--board",
        "amd64-generic"
      ]
    ],
    "name": "build packages"
  }
]