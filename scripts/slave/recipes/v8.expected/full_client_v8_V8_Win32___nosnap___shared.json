[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]\\scripts\\slave\\kill_processes.py"
    ],
    "name": "taskkill"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]\\gclient.py",
      "config",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'v8', 'url': 'http://v8.googlecode.com/svn/branches/bleeding_edge'}]"
    ],
    "name": "gclient setup"
  },
  {
    "abort_on_failure": true,
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]\\gclient.py",
      "sync",
      "--nohooks",
      "--delete_unversioned_trees",
      "--force",
      "--output-json",
      "/path/to/tmp/json"
    ],
    "name": "gclient sync",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"solutions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"v8/\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": 50110@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@50110@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]\\gclient.py",
      "runhooks"
    ],
    "env": {
      "GYP_DEFINES": "component=shared_library target_arch=ia32 v8_target_arch=ia32 v8_use_snapshot=false",
      "GYP_GENERATORS": "msvs",
      "GYP_GENERATOR_FLAGS": "msvs_error_on_missing_sources=1",
      "GYP_MSVS_VERSION": "2010"
    },
    "name": "gclient runhooks",
    "seed_steps": [
      "gclient runhooks",
      "cleanup_temp",
      "compile",
      "Check",
      "Check - flaky"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]\\scripts\\slave\\cleanup_temp.py"
    ],
    "name": "cleanup_temp"
  },
  {
    "abort_on_failure": true,
    "cmd": [
      "python",
      "-u",
      "[BUILD]\\scripts\\slave\\compile.py",
      "--target",
      "Release",
      "--src-dir",
      "[SLAVE_BUILD]\\v8",
      "--build-tool",
      "msvs",
      "--"
    ],
    "name": "compile"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]\\scripts\\slave\\v8\\v8testing.py",
      "--target",
      "Release",
      "--arch",
      "ia32",
      "--testname",
      "mjsunit cctest message preparser",
      "--flaky-tests",
      "skip"
    ],
    "cwd": "[SLAVE_BUILD]\\v8",
    "name": "Check"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]\\scripts\\slave\\v8\\v8testing.py",
      "--target",
      "Release",
      "--arch",
      "ia32",
      "--testname",
      "mjsunit cctest message preparser",
      "--flaky-tests",
      "run"
    ],
    "cwd": "[SLAVE_BUILD]\\v8",
    "name": "Check - flaky"
  }
]