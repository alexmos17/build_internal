[
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS_ROOT]/gclient.py",
      "config",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'v8', 'url': 'http://v8.googlecode.com/svn/branches/bleeding_edge'}]"
    ],
    "name": "gclient setup",
    "seed_steps": [
      "gclient setup",
      "gclient revert",
      "gclient sync"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD_ROOT]/scripts/slave/gclient_safe_revert.py",
      ".",
      "[DEPOT_TOOLS_ROOT]/gclient"
    ],
    "name": "gclient revert"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS_ROOT]/gclient.py",
      "sync",
      "--nohooks"
    ],
    "name": "gclient sync"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS_ROOT]/gclient.py",
      "runhooks"
    ],
    "env": {
      "GYP_DEFINES": "v8_target_arch=ia32"
    },
    "name": "gclient runhooks"
  },
  {
    "abort_on_failure": true,
    "cmd": [
      "python",
      "-u",
      "[BUILD_ROOT]/scripts/slave/compile.py",
      "--target",
      "Release",
      "--build-dir",
      "[SLAVE_BUILD_ROOT]/v8",
      "--src-dir",
      "[SLAVE_BUILD_ROOT]/v8",
      "--build-tool",
      "make",
      "--clobber",
      "--",
      "buildbot"
    ],
    "name": "compile"
  }
]