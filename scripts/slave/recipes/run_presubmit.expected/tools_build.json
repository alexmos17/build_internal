[
  {
    "abort_on_failure": true,
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "tryserver.chromium",
      "--builder",
      "chromium_presubmit",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = '[ROOT]/git_cache'\nsolutions = [{'deps_file': '.DEPS.git', 'managed': True, 'name': 'build', 'url': 'https://chromium.googlesource.com/chromium/tools/build.git'}]",
      "--root",
      "build",
      "--revision",
      "HEAD",
      "--revision_mapping_file",
      "{\"build\": \"got_revision\"}",
      "--issue",
      "12853011",
      "--patchset",
      "1",
      "--output_json",
      "/path/to/tmp/json"
    ],
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"build\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision\": 270553, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_git\": \"80754af91bfb6d1073585b046fe0a474ce868509\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"build\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@270553@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_git@\"80754af91bfb6d1073585b046fe0a474ce868509\"@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "commit",
      "-c",
      "user.email=commit-bot@chromium.org",
      "-c",
      "user.name=The Commit Bot",
      "-a",
      "-m",
      "Committed patch"
    ],
    "cwd": "[SLAVE_BUILD]/build/build",
    "name": "git commit"
  },
  {
    "cmd": [
      "git",
      "config",
      "user.email",
      "commit-bot@chromium.org"
    ],
    "cwd": "[SLAVE_BUILD]/build",
    "name": "git config user.email",
    "seed_steps": [
      "git config user.email",
      "git config user.name",
      "git clean"
    ]
  },
  {
    "cmd": [
      "git",
      "config",
      "user.name",
      "The Commit Bot"
    ],
    "cwd": "[SLAVE_BUILD]/build",
    "name": "git config user.name"
  },
  {
    "cmd": [
      "git",
      "clean",
      "-xfq"
    ],
    "cwd": "[SLAVE_BUILD]/build",
    "name": "git clean"
  },
  {
    "cmd": [
      "[DEPOT_TOOLS]/presubmit_support.py",
      "--root",
      "[SLAVE_BUILD]/build/build",
      "--commit",
      "--verbose",
      "--verbose",
      "--issue",
      "12853011",
      "--patchset",
      "1",
      "--skip_canned",
      "CheckRietveldTryJobExecution",
      "--skip_canned",
      "CheckTreeIsOpen",
      "--skip_canned",
      "CheckBuildbotPendingBuilds",
      "--rietveld_url",
      "https://codereview.chromium.org",
      "--rietveld_email",
      "",
      "--rietveld_fetch",
      "--upstream",
      "80754af91bfb6d1073585b046fe0a474ce868509",
      "--trybot-json",
      "/path/to/tmp/json"
    ],
    "name": "presubmit",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  [@@@",
      "@@@STEP_LOG_LINE@json.output@    \"chromium_presubmit\", @@@",
      "@@@STEP_LOG_LINE@json.output@    [@@@",
      "@@@STEP_LOG_LINE@json.output@      \"compile\"@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  }
]